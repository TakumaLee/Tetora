{
  "manifest_version": 3,
  "name": "Tetora Browser Relay",
  "version": "1.0.0",
  "description": "Connects your browser to Tetora AI agent for automated browsing",
  "permissions": ["activeTab", "tabs", "scripting"],
  "host_permissions": ["http://127.0.0.1:18792/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
